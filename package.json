{
  "name": "ssvproff",
  "version": "1.0.0",
  "description": "Minimal starter with documentation, automation, and API/Web scaffolding",
  "private": true,
  "main": "index.js",
  "scripts": {
    "dev": "concurrently \"cd packages/api && pnpm dev\" \"cd packages/web && pnpm dev\"",
    "build": "pnpm --recursive build",
    "build:api": "cd packages/api && pnpm build",
    "build:web": "cd packages/web && pnpm build",
    "lint": "pnpm --recursive lint"
  },
  "keywords": ["typescript", "monorepo", "ssvproff", "api", "web"],
  "author": "SSV Platform Team",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^8.2.2",
    "typescript": "^5.0.0",
    "@types/node": "^20.0.0"
  },
  "packageManager": "pnpm@9.1.0"
}
